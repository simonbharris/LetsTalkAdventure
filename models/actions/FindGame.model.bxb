action (FindGame) {
  description (Finds a game)
  type (Search)
  collect {
    input (name) {
      type (Name)
      min (Optional) max (One)
    }
    input (platforms) {
      type (PlatformNames)
      min (Optional) max (Many)
    }
    input (genres) {
      type (GenreNames)
      min (Optional) max (Many)
    }
    input (themes) {
      type(ThemeNames)
      min (Optional) max (Many)
    }
  }
  output (Game)
}
